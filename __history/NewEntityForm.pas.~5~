{$R *.dfm}

procedure TnewEntityFrm.CreateEntityButtonClick(Sender: TObject);
var
    soapClient2 : IBusinessAPI;
    EntityIDInt : Integer ;
    I           : Integer;
    strValue    : string;
    EntityAddresponse : string;
begin
      strValue := EntityIDEdit.Text;
      Val(strValue, I, EntityIDInt);

      EntityAddresponse:=soapClient2.Entity_Add
      (
      strtoint(EntityIDEdit.Text),
      UserNameEdit.Text,
      PasswordOlEdit.Text,
      strtoint(BusinessIDEdit.Text),
      null,
      null,
      EmailEdit.Text,
      PasswordEdit.Text,
      FirstNameEdit.Text,
      LastNameEdit.Text,
      MobileEdit.Text,
      CountryISOEdit.Text,
      null ) ;

      strValue :='adasds';



         // <BusinessId xsi:type="xsd:int">1</BusinessId>
        // <ol_EntityId xsi:type="xsd:int">1</ol_EntityId>
        // <ol_UserName xsi:type="xsd:string">admin</ol_UserName>
        // <ol_Password xsi:type="xsd:string">admin</ol_Password>
        // <Email xsi:type="xsd:string">pashayev.ilkin1@gmail.com</Email>
        // <Password xsi:type="xsd:string">admin</Password>
        // <FirstName xsi:type="xsd:string">Ilkin</FirstName>
        // <LastName xsi:type="xsd:string">Pashayev</LastName>
        // <Mobile xsi:type="xsd:string">+99451900185</Mobile>
        // <CountryISO xsi:type="xsd:string">IL</CountryISO>

end;

end.
